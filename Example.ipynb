   "source": [
    "def myfunc(string):\n",
    "    new_string = ''\n",
    "    for x in range(len(string)):\n",
    "        if (x+1)%2==0:\n",
    "            new_string += string[x].upper()\n",
    "        else:\n",
    "            new_string += string[x].lower()\n",
    "    return new_string"
   ]
